{
  "name": "impersonate-agents",
  "displayName": "Tool for Impersonating Agents",
  "version": "1.1.0",
  "description": "A simple chrome extension for impersonate a agent.",
  "type": "module",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.15.0",
    "plasmo": "^0.90.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.268",
    "@types/node": "^20.14.15",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.23",
    "daisyui": "^4.12.14",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.5.4"
  },
  "manifest": {
    "host_permissions": [
      "http://*/*",
      "https://*/*"
